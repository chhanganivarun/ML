#include<stdio.h>

int main()
{
	char filename[101];
	printf("Please Enter Filename for the dataset:");
	scanf("%[^\n]s",filename);

	FILE *datain = fopen(filename,"r");
	if ( datain == NULL )
	{
		printf("Error occurred while taking input from dataset.\nFile not found or doesn't have permissions\n");
	}
	fclose(datain);
}
